    "postpublish": "node ../../scripts/builds/restore-package.mjs"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4"
  }
