{
  "name": "@viso/base",
  "version": "0.0.1",
  "private": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --ext ts,tsx,js,jsx src",
